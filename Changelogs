local ChangelogsSection3 = getgenv().ChangelogsTab:Section({Side = "Right"});
ChangelogsSection3:Header({Text = "Suggestion Reports"});

ChangelogsSection3:Divider();

ChangelogsSection3:Paragraph({Header = "* Before you write:",Body = "Make sure to state the game you want, It is crucial."});

ChangelogsSection3:Input({Name = "Type here whatever~",Placeholder = 'Write here',AcceptedCharacters = "All",
    Callback = function(Input)
        getgenv().SuggestionValue = Input;
    end,
},"SuggestInput");

ChangelogsSection3:Button({Name = "Submit Suggestion [LOCKED]",Callback = function()
   -- getgenv().SubmitSuggestion();
end});

ChangelogsSection3:SubLabel({Text = "Disclaimer: Usernames will be logged."});
